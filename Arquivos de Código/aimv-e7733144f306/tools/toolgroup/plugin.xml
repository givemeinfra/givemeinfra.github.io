<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="aimv.tools">
      <tool
            class="tools.GroupsTool"
            id="tools.groupstool"
            name="Groups Tool">
      </tool>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="tools.actionset.groups"
            label="ActionSet"
            visible="true">
         <action
               class="tools.actions.GroupsToolAction"
               icon="icons/groups.png"
               id="tools.actions.groupstool"
               label="Groups"
               style="pulldown"
               toolbarPath="Normal">
         </action>
      </actionSet>
   </extension>


</plugin>
