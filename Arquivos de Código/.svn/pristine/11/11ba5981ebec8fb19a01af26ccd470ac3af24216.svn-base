package aimv.core;

public final class Number {
	
	public static String parseBRtoUS(String br) {
		
		String us = null;
		if (br == null)
			return us;;
			
		String[] array = br.split(",");
		if (array.length > 1)
			us = array[0] + "." + array[1];
		else
			us = array[0];
		return us;
		
	}//parseBRtoUS

}
